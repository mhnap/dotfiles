{
	"accessibility.signals.taskCompleted": {
		"sound": "on"
	},
	"accessibility.signals.taskFailed": {
		"sound": "on"
	},
	"diffEditor.hideUnchangedRegions.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.renderSideBySide": false,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": false,
	"editor.fontFamily": "JetBrains Mono",
	"editor.guides.indentation": false,
	"editor.hover.delay": 1000,
	"editor.inlayHints.enabled": "offUnlessPressed",
	"editor.lineHeight": 1.6,
	"editor.matchBrackets": "near",
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "none",
	"editor.scrollBeyondLastLine": false,
	"editor.semanticTokenColorCustomizations": {
		"[Monokai]": {
			"rules": {
				"*.unsafe": {
					"fontStyle": "underline"
				},
				"builtinType": {
					"fontStyle": "",
					"foreground": "#F92672"
				},
				"comment": "#75715E",
				"const": "#908B25",
				"decorator": "#F8F8F2",
				"decorator.declaration": "#A6E22E",
				"derive": {
					"fontStyle": "",
					"foreground": "#6897BB"
				},
				"enum": {
					"fontStyle": "",
					"foreground": "#B5B6E3"
				},
				"enumMember": {
					"fontStyle": "italic",
					"foreground": "#B5B6E3"
				},
				"formatSpecifier": "#4EADE5",
				"function": "#66D9EF",
				"function.declaration": "#A6E22E",
				"interface": {
					"fontStyle": "",
					"foreground": "#6897BB"
				},
				"keyword": {
					"fontStyle": ""
				},
				"lifetime": {
					"fontStyle": "italic",
					"foreground": "#20999D"
				},
				"macro": "#4EADE5",
				"method": "#66D9EF",
				"method.declaration": "#A6E22E",
				"namespace": {
					"fontStyle": "",
					"foreground": "#B5B6E3"
				},
				"parameter": {
					"fontStyle": ""
				},
				"property": "#AE81FF",
				"static": "#908B25",
				"struct": {
					"fontStyle": "",
					"foreground": "#B5B6E3"
				},
				"typeAlias": {
					"fontStyle": "",
					"foreground": "#B5B6E3"
				},
				"typeParameter": {
					"fontStyle": "",
					"foreground": "#20999D"
				},
				"variable.constant": "#908B25",
				"variable.declaration.constant": "#908B25",
				"variable.declaration.static": "#908B25",
				"variable.static": "#908B25"
			}
		}
	},
	"editor.smoothScrolling": true,
	"editor.stickyScroll.enabled": false,
	"editor.tokenColorCustomizations": {
		"[Monokai]": {
			"textMateRules": [
				{
					"scope": "comment",
					"settings": {
						"foreground": "#75715E"
					}
				},
				{
					"scope": "constant.other",
					"settings": {
						"foreground": "#908B25"
					}
				},
				{
					"scope": "entity.name.function",
					"settings": {
						"foreground": "#66D9EF"
					}
				},
				{
					"scope": "entity.name.function.macro",
					"settings": {
						"foreground": "#4EADE5"
					}
				},
				{
					"scope": "entity.name.namespace",
					"settings": {
						"fontStyle": "",
						"foreground": "#B5B6E3"
					}
				},
				{
					"scope": "entity.name.type",
					"settings": {
						"fontStyle": "",
						"foreground": "#B5B6E3"
					}
				},
				{
					"scope": "entity.name.type.lifetime",
					"settings": {
						"fontStyle": "italic",
						"foreground": "#20999D"
					}
				},
				{
					"scope": "entity.name.type.numeric",
					"settings": {
						"foreground": "#F92672"
					}
				},
				{
					"scope": "entity.name.type.primitive",
					"settings": {
						"foreground": "#F92672"
					}
				},
				{
					"scope": "keyword.operator.access.dot",
					"settings": {
						"foreground": "#F8F8F2"
					}
				},
				{
					"scope": "punctuation.brackets.angle",
					"settings": {
						"foreground": "#F92672"
					}
				},
				{
					"scope": "punctuation.definition.interpolation",
					"settings": {
						"foreground": "#4EADE5"
					}
				},
				{
					"scope": "punctuation.definition.lifetime",
					"settings": {
						"fontStyle": "italic",
						"foreground": "#20999D"
					}
				},
				{
					"scope": "storage.type",
					"settings": {
						"fontStyle": "",
						"foreground": "#F92672"
					}
				},
				{
					"scope": "variable.language.self",
					"settings": {
						"foreground": "#F92672"
					}
				}
			]
		}
	},
	"explorer.confirmDelete": false,
	"files.autoSave": "afterDelay",
	"files.exclude": {
		"target/**": true
	},
	"files.watcherExclude": {
		"target/**": true
	},
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"github.copilot.editor.enableAutoCompletions": false,
	"githubPullRequests.pullBranch": "never",
	"gitlens.codeLens.enabled": false,
	"gitlens.currentLine.enabled": false,
	"rust-analyzer.cargo.allTargets": false,
	"rust-analyzer.check.allTargets": false,
	"rust-analyzer.check.workspace": false,
	"rust-analyzer.checkOnSave": false,
	"rust-analyzer.lens.implementations.enable": false,
	"rust-analyzer.runnables.askBeforeUpdateTest": false,
	"rust-analyzer.semanticHighlighting.operator.enable": false,
	"search.showLineNumbers": true,
	"security.promptForLocalFileProtocolHandling": false,
	"security.workspace.trust.untrustedFiles": "open",
	"window.menuBarVisibility": "hidden",
	"window.newWindowDimensions": "fullscreen",
	"window.restoreFullscreen": true,
	"workbench.activityBar.location": "top",
	"workbench.colorCustomizations": {
		"[Monokai]": {
			"statusBarItem.remoteBackground": "#414339"
		}
	},
	"workbench.colorTheme": "Monokai",
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.perEditorGroup": true,
	"workbench.editor.limit.value": 20,
	"workbench.list.horizontalScrolling": true,
	"workbench.list.smoothScrolling": true,
	"workbench.tree.renderIndentGuides": "none"
}